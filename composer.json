{
  "name": "contributte/middlewares",
  "description": "Middleware / Relay / PSR-7 support to Nette Framework",
  "keywords": [
    "middleware",
    "relay",
    "nette"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/middlewares",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/di": "^0.5.1",
    "contributte/psr7-http-message": "^0.8.0"
  },
  "require-dev": {
    "nette/application": "^3.0.0",
    "nette/http": "^3.0.1",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.13",
    "phpstan/phpstan": "^1.6.8",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.2.3",
    "psr/log": "^1.0|^2.0|^3.0",
    "tracy/tracy": "^2.7.0"
  },
  "suggest": {
    "tracy/tracy": "to use TracyMiddleware",
    "nette/http": "to use NetteMiddlewareExtension & NetteMiddlewareApplication",
    "nette/application": "to use PresenterMiddleware"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Middlewares\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.11.x-dev"
    }
  }
}
